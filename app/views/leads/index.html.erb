<div class="mt-5">
  <h1>Leads console</h1>
</div>
<div class="scroll-card">
  <ul class="list-group">
    <% @leads.each do |lead| %>
      <li class="list-group-item">
        <%= lead.customer.name %>
      </li>
    <% end %>
  </ul>
</div>
<div class="mt-2">
  <%= link_to "New Lead", new_lead_path, class: "btn btn-flat" %>
</div>
