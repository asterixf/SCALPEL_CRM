<div class="container">
  <div class="mt-5">
    <h1>Leads console</h1>
  </div>
  <div class="scroll-card">
    <ul class="list-group">
      <% @leads.each do |lead| %>
        <li class="list-group-item">
          <p><%= lead.customer.name%> Lead <%= lead.id %></p>
        </li>
      <% end %>
    </ul>
  </div>
</div>
